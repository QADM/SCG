<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE report PUBLIC "-//JFreeReport//DTD report definition//EN//simple/version 0.8.5" "http://jfreereport.sourceforge.net/report-085.dtd">

<report bottommargin="10" topmargin="10" rightmargin="10" leftmargin="10" pageformat="LETTER" orientation="landscape" name="TestReport">
  <parser-config>
    <property name="reportheader">
      <!-- label -->
    </property>
    <property name="reportfooter">
      <!-- label -->
    </property>
    <property name="pageheader">
      <!-- label -->
    </property>
    <property name="pagefooter">
      <!-- label -->
    </property>
  </parser-config>
  <reportheader color="#133484" fontname="Arial" fontsize="16">
    <message-field alignment="left" height="24" vertical-alignment="middle" width="100%" x="0%" y="0">${reportheader}</message-field>
  </reportheader>
  <reportfooter color="#133484" fontname="Arial" fontsize="16">
    <message-field alignment="left" height="24" vertical-alignment="middle" width="100%" x="0%" y="0">${reportfooter}</message-field>
  </reportfooter>
  <pageheader color="#32384C" fontname="Arial" fontsize="8">
    <round-rectangle height="18" arc-height="6" arc-width="6" color="#7EBAD2" width="100%" x="0%" y="0" fill="true" draw="true" dynamic="false" layout-cachable="false"/>
    <message-field alignment="left" height="18" vertical-alignment="middle" width="50%" x="0%" y="0">${pageheader}</message-field>
    <message-field alignment="right" height="18" vertical-alignment="middle" width="50%" x="50%" y="0">$(report.date, date, MMMM dd, yyyy @ hh:mm )</message-field>
    <line color="#133346" height="3" weight="3" width="100%" x1="0%" x2="100%" y1="18" y2="18"/>
  </pageheader>
  <pagefooter color="#32384C" fontname="Arial" fontsize="8">
    <round-rectangle height="18" arc-height="6" arc-width="6" color="#8689A0" width="100%" x="0%" y="0" fill="true" draw="true" dynamic="false" layout-cachable="false"/>
    <line color="#133346" height="3" weight="3" width="100%" x1="0%" x2="100%" y1="18" y2="18"/>
    <message-field alignment="left" height="18" width="50%" x="0" y="0">${pagefooter}</message-field>
    <message-field alignment="right" height="18" width="50%" x="50%" y="0">Page $(PageofPages)</message-field>
  </pagefooter>
  <functions>
    <property-ref name="report.date"/>
    <function class="org.jfree.report.function.PageOfPagesFunction" name="PageofPages"/>
    <function name="backgroundTrigger" class="org.jfree.report.function.ElementVisibilitySwitchFunction">
      <properties>
        <property name="element">rowBandingElement</property>
        <property name="numberOfElements">1</property>
        <property name="initialState">true</property>
      </properties>
    </function>
    <expression class="org.jfree.report.function.HideElementIfDataAvailableExpression" name="noDataLabelElementHideIfNullTrigger">
      <properties>
        <property name="element">noDataLabelElement</property>
      </properties>
    </expression>
    <expression class="org.jfree.report.function.ShowElementIfDataAvailableExpression" name="dummyGroupFooterHideIfNullTrigger">
      <properties>
        <property name="element">dummyGroupFooter</property>
      </properties>
    </expression>
    <expression class="org.jfree.report.function.ShowElementIfDataAvailableExpression" name="LC_INLINE_SQL_1_pc_CuentaGroupHeaderHideIfNullTrigger">
      <properties>
        <property name="element">LC_INLINE_SQL_1_pc_CuentaGroupHeader</property>
      </properties>
    </expression>
    <expression class="org.jfree.report.function.ShowElementIfDataAvailableExpression" name="LC_INLINE_SQL_1_pc_CuentaGroupFooterHideIfNullTrigger">
      <properties>
        <property name="element">LC_INLINE_SQL_1_pc_CuentaGroupFooter</property>
      </properties>
    </expression>
    <expression class="org.jfree.report.function.ShowElementIfDataAvailableExpression" name="LC_INLINE_SQL_1_pc_NombreGroupHeaderHideIfNullTrigger">
      <properties>
        <property name="element">LC_INLINE_SQL_1_pc_NombreGroupHeader</property>
      </properties>
    </expression>
    <expression class="org.jfree.report.function.ShowElementIfDataAvailableExpression" name="LC_INLINE_SQL_1_pc_NombreGroupFooterHideIfNullTrigger">
      <properties>
        <property name="element">LC_INLINE_SQL_1_pc_NombreGroupFooter</property>
      </properties>
    </expression>
    <expression class="org.jfree.report.function.ShowElementIfDataAvailableExpression" name="LC_INLINE_SQL_1_pc_Fecha_ContableGroupHeaderHideIfNullTrigger">
      <properties>
        <property name="element">LC_INLINE_SQL_1_pc_Fecha_ContableGroupHeader</property>
      </properties>
    </expression>
    <expression class="org.jfree.report.function.ShowElementIfDataAvailableExpression" name="LC_INLINE_SQL_1_pc_Fecha_ContableGroupFooterHideIfNullTrigger">
      <properties>
        <property name="element">LC_INLINE_SQL_1_pc_Fecha_ContableGroupFooter</property>
      </properties>
    </expression>
  </functions>
  <configuration>
    <property name="org.pentaho.reporting.engine.classic.core.modules.output.table.html.AssumeOverflowX">false</property>
    <property name="org.pentaho.reporting.engine.classic.core.modules.output.table.html.AssumeOverflowY">true</property>
  </configuration>
  <items color="#4F383C" fontname="Arial" fontsize="10" fontstyle="plain">
    <rectangle name="rowBandingElement" color="#9CD8F1" draw="false" fill="true" height="12" x="0%" width="100%" y="0"/>
    <string-field name="LC_INLINE_SQL_1_pc_DescripcionElement" nullstring="-" fieldname="LC_INLINE_SQL_1_pc_Descripcion" vertical-alignment="middle" alignment="left" width="25.0%" x="0.0%" y="0" height="12" color="#4F383C" fontname="Arial" fontsize="10" fontstyle="plain"/>
    <line color="#7F887B" weight=".5" x1="0.0%" y1="0" x2="0.0%" y2="12"/>
    <line color="#7F887B" weight=".5" x1="100%" y1="0" x2="100%" y2="12"/>
    <line color="#7F887B" weight=".5" x1="0%" y1="0" x2="100%" y2="0"/>
    <line color="#7F887B" weight=".5" x1="0%" y1="12" x2="100%" y2="12"/>
  </items>
  <groups>
    <group name="dummy">
      <groupheader name="dummyGroupHeader" color="#1A7439" fontname="Arial" fontsize="12" fontstyle="bold" repeat="false">
        <label name="noDataLabelElement" height="12" vertical-alignment="middle" alignment="left" width="100%" x="0%" y="0" visible="false">No Data Available</label>
      </groupheader>
      <groupfooter name="dummyGroupFooter" color="#1A7439" fontname="Arial" fontsize="12" fontstyle="bold"/>
    </group>
    <group name="LC_INLINE_SQL_1_pc_Cuenta">
      <fields>
        <field>LC_INLINE_SQL_1_pc_Cuenta</field>
      </fields>
      <groupheader name="LC_INLINE_SQL_1_pc_CuentaGroupHeader" repeat="false" pagebreak-before-print="false" pagebreak-after-print="false" color="#1A7439" fontname="Arial" fontsize="12" fontstyle="bold">
        <rectangle color="#FFFFFF" draw="false" fill="false" height="17" width="100%" x="0%" y="0"/>
        <message-field height="17" vertical-alignment="top" alignment="left" width="100%" x="0%" y="0" nullstring="-">Cuenta: $(LC_INLINE_SQL_1_pc_Cuenta)</message-field>
      </groupheader>
      <groupfooter name="LC_INLINE_SQL_1_pc_CuentaGroupFooter" color="#1A7439" fontname="Arial" fontsize="12" fontstyle="bold" pagebreak-before-print="false" pagebreak-after-print="false"/>
    </group>
    <group name="LC_INLINE_SQL_1_pc_Nombre">
      <fields>
        <field>LC_INLINE_SQL_1_pc_Cuenta</field>
        <field>LC_INLINE_SQL_1_pc_Nombre</field>
      </fields>
      <groupheader name="LC_INLINE_SQL_1_pc_NombreGroupHeader" repeat="false" pagebreak-before-print="false" pagebreak-after-print="false" color="#1A7439" fontname="Arial" fontsize="12" fontstyle="bold">
        <rectangle color="#FFFFFF" draw="false" fill="false" height="17" width="100%" x="0%" y="0"/>
        <message-field height="17" vertical-alignment="top" alignment="left" width="100%" x="0%" y="0" nullstring="-">Nombre: $(LC_INLINE_SQL_1_pc_Nombre)</message-field>
      </groupheader>
      <groupfooter name="LC_INLINE_SQL_1_pc_NombreGroupFooter" color="#1A7439" fontname="Arial" fontsize="12" fontstyle="bold" pagebreak-before-print="false" pagebreak-after-print="false"/>
    </group>
    <group name="LC_INLINE_SQL_1_pc_Fecha_Contable">
      <fields>
        <field>LC_INLINE_SQL_1_pc_Cuenta</field>
        <field>LC_INLINE_SQL_1_pc_Nombre</field>
        <field>LC_INLINE_SQL_1_pc_Fecha_Contable</field>
      </fields>
      <groupheader name="LC_INLINE_SQL_1_pc_Fecha_ContableGroupHeader" repeat="false" pagebreak-before-print="false" pagebreak-after-print="false" color="#1A7439" fontname="Arial" fontsize="12" fontstyle="bold" height="43">
        <rectangle color="#FFFFFF" draw="false" fill="false" height="17" width="100%" x="0%" y="0"/>
        <message-field height="17" vertical-alignment="top" alignment="left" width="100%" x="0%" y="0" nullstring="-">Fecha_Contable: $(LC_INLINE_SQL_1_pc_Fecha_Contable)</message-field>
        <rectangle color="#133346" draw="false" fill="true" height="18" width="100%" x="0%" y="19"/>
        <rectangle draw="false" fill="false" height="2" width="100%" x="0%" y="17"/>
        <label color="#5B778D" fontname="Arial" fontsize="10" fontstyle="plain" height="18" vertical-alignment="middle" alignment="left" width="25.0%" x="0.0%" y="19">Descripcion</label>
      </groupheader>
      <groupfooter name="LC_INLINE_SQL_1_pc_Fecha_ContableGroupFooter" color="#1A7439" fontname="Arial" fontsize="12" fontstyle="bold" pagebreak-before-print="false" pagebreak-after-print="false"/>
    </group>
  </groups>
</report>
