	id: 1,
    type: "query",
    name: "Test 1",
    group: "CDV Sample Tests",
	path: "/cdv/tests/test.cdv",
	createdBy: "Pedro",
	createdAt: 1339430893246,
    queries: [ 
    {
        cdaFile: "/plugin-samples/cda/cdafiles/sql-jndi.cda", 
        dataAccessId: "1" , 
