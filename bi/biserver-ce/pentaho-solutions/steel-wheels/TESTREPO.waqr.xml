<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE report PUBLIC "-//JFreeReport//DTD report definition//EN//simple/version 0.8.5" "http://jfreereport.sourceforge.net/report-085.dtd">

<report bottommargin="10" topmargin="10" rightmargin="10" leftmargin="10" pageformat="LETTER" orientation="landscape" name="TESTREPO">
  <parser-config>
    <property name="reportheader">
      <!-- label -->
    </property>
    <property name="reportfooter">
      <!-- label -->
    </property>
    <property name="pageheader">
      <!-- label -->
    </property>
    <property name="pagefooter">
      <!-- label -->
    </property>
  </parser-config>
  <reportheader color="#C03021" fontname="Arial" fontsize="16">
    <message-field alignment="left" height="24" vertical-alignment="middle" width="100%" x="0%" y="0">${reportheader}</message-field>
  </reportheader>
  <reportfooter color="#C03021" fontname="Arial" fontsize="16">
    <message-field alignment="left" height="24" vertical-alignment="middle" width="100%" x="0%" y="0">${reportfooter}</message-field>
  </reportfooter>
  <pageheader color="#2F2C35" fontname="Arial" fontsize="8">
    <round-rectangle height="18" arc-height="6" arc-width="6" color="#B4805F" width="100%" x="0%" y="0" fill="true" draw="true" dynamic="false" layout-cachable="false"/>
    <message-field alignment="left" height="18" vertical-alignment="middle" width="50%" x="0%" y="0">${pageheader}</message-field>
    <message-field alignment="right" height="18" vertical-alignment="middle" width="50%" x="50%" y="0">$(report.date, date, MMMM dd, yyyy @ hh:mm )</message-field>
    <line color="#C03021" height="3" weight="3" width="100%" x1="0%" x2="100%" y1="18" y2="18"/>
  </pageheader>
  <pagefooter color="#2F2C35" fontname="Arial" fontsize="8">
    <round-rectangle height="18" arc-height="6" arc-width="6" color="#B4805F" width="100%" x="0%" y="0" fill="true" draw="true" dynamic="false" layout-cachable="false"/>
    <line color="#C03021" height="3" weight="3" width="100%" x1="0%" x2="100%" y1="18" y2="18"/>
    <message-field alignment="left" height="18" width="50%" x="0" y="0">${pagefooter}</message-field>
    <message-field alignment="right" height="18" width="50%" x="50%" y="0">Page $(PageofPages)</message-field>
  </pagefooter>
  <functions>
    <property-ref name="report.date"/>
    <function class="org.jfree.report.function.PageOfPagesFunction" name="PageofPages"/>
    <function name="backgroundTrigger" class="org.jfree.report.function.ElementVisibilitySwitchFunction">
      <properties>
        <property name="element">rowBandingElement</property>
        <property name="numberOfElements">1</property>
        <property name="initialState">true</property>
      </properties>
    </function>
    <expression class="org.jfree.report.function.HideElementIfDataAvailableExpression" name="noDataLabelElementHideIfNullTrigger">
      <properties>
        <property name="element">noDataLabelElement</property>
      </properties>
    </expression>
    <expression class="org.jfree.report.function.ShowElementIfDataAvailableExpression" name="dummyGroupFooterHideIfNullTrigger">
      <properties>
        <property name="element">dummyGroupFooter</property>
      </properties>
    </expression>
    <expression class="org.jfree.report.function.ShowElementIfDataAvailableExpression" name="LC_INLINE_SQL_1_pc_ACCTG_TRANS_IDGroupHeaderHideIfNullTrigger">
      <properties>
        <property name="element">LC_INLINE_SQL_1_pc_ACCTG_TRANS_IDGroupHeader</property>
      </properties>
    </expression>
    <expression class="org.jfree.report.function.ShowElementIfDataAvailableExpression" name="LC_INLINE_SQL_1_pc_ACCTG_TRANS_IDGroupFooterHideIfNullTrigger">
      <properties>
        <property name="element">LC_INLINE_SQL_1_pc_ACCTG_TRANS_IDGroupFooter</property>
      </properties>
    </expression>
    <expression class="org.jfree.report.function.ShowElementIfDataAvailableExpression" name="LC_INLINE_SQL_1_pc_CICLOGroupHeaderHideIfNullTrigger">
      <properties>
        <property name="element">LC_INLINE_SQL_1_pc_CICLOGroupHeader</property>
      </properties>
    </expression>
    <expression class="org.jfree.report.function.ShowElementIfDataAvailableExpression" name="LC_INLINE_SQL_1_pc_CICLOGroupFooterHideIfNullTrigger">
      <properties>
        <property name="element">LC_INLINE_SQL_1_pc_CICLOGroupFooter</property>
      </properties>
    </expression>
  </functions>
  <configuration>
    <property name="org.pentaho.reporting.engine.classic.core.modules.output.table.html.AssumeOverflowX">false</property>
    <property name="org.pentaho.reporting.engine.classic.core.modules.output.table.html.AssumeOverflowY">true</property>
  </configuration>
  <items color="#000000" fontname="Arial" fontsize="10" fontstyle="plain">
    <rectangle name="rowBandingElement" color="#F1DD7E" draw="false" fill="true" height="12" x="0%" width="100%" y="0"/>
    <string-field name="LC_INLINE_SQL_1_pc_CONElement" nullstring="-" fieldname="LC_INLINE_SQL_1_pc_CON" vertical-alignment="middle" alignment="left" width="33.0%" x="0.0%" y="0" height="12" color="#000000" fontname="Arial" fontsize="10" fontstyle="plain"/>
    <line color="#696778" weight=".5" x1="0.0%" y1="0" x2="0.0%" y2="12"/>
    <line color="#696778" weight=".5" x1="100%" y1="0" x2="100%" y2="12"/>
    <line color="#696778" weight=".5" x1="0%" y1="0" x2="100%" y2="0"/>
    <line color="#696778" weight=".5" x1="0%" y1="12" x2="100%" y2="12"/>
  </items>
  <groups>
    <group name="dummy">
      <groupheader name="dummyGroupHeader" color="black" fontname="Arial" fontsize="12" fontstyle="bold" repeat="false">
        <label name="noDataLabelElement" height="12" vertical-alignment="middle" alignment="left" width="100%" x="0%" y="0" visible="false">No Data Available</label>
      </groupheader>
      <groupfooter name="dummyGroupFooter" color="black" fontname="Arial" fontsize="12" fontstyle="bold"/>
    </group>
    <group name="LC_INLINE_SQL_1_pc_ACCTG_TRANS_ID">
      <fields>
        <field>LC_INLINE_SQL_1_pc_ACCTG_TRANS_ID</field>
      </fields>
      <groupheader name="LC_INLINE_SQL_1_pc_ACCTG_TRANS_IDGroupHeader" repeat="false" pagebreak-before-print="false" pagebreak-after-print="false" color="black" fontname="Arial" fontsize="12" fontstyle="bold">
        <rectangle color="#FFFFFF" draw="false" fill="false" height="17" width="100%" x="0%" y="0"/>
        <message-field height="17" vertical-alignment="top" alignment="left" width="100%" x="0%" y="0" nullstring="-">ACCTG_TRANS_ID: $(LC_INLINE_SQL_1_pc_ACCTG_TRANS_ID)</message-field>
      </groupheader>
      <groupfooter name="LC_INLINE_SQL_1_pc_ACCTG_TRANS_IDGroupFooter" color="black" fontname="Arial" fontsize="12" fontstyle="bold" pagebreak-before-print="false" pagebreak-after-print="false"/>
    </group>
    <group name="LC_INLINE_SQL_1_pc_CICLO">
      <fields>
        <field>LC_INLINE_SQL_1_pc_ACCTG_TRANS_ID</field>
        <field>LC_INLINE_SQL_1_pc_CICLO</field>
      </fields>
      <groupheader name="LC_INLINE_SQL_1_pc_CICLOGroupHeader" repeat="false" pagebreak-before-print="false" pagebreak-after-print="false" color="black" fontname="Arial" fontsize="12" fontstyle="bold" height="43">
        <rectangle color="#FFFFFF" draw="false" fill="false" height="17" width="100%" x="0%" y="0"/>
        <message-field height="17" vertical-alignment="top" alignment="left" width="100%" x="0%" y="0" nullstring="-">CICLO: $(LC_INLINE_SQL_1_pc_CICLO)</message-field>
        <rectangle color="#AB971D" draw="false" fill="true" height="18" width="100%" x="0%" y="19"/>
        <rectangle draw="false" fill="false" height="2" width="100%" x="0%" y="17"/>
        <label color="#C03021" fontname="Arial" fontsize="10" fontstyle="plain" height="18" vertical-alignment="middle" alignment="left" width="33.0%" x="0.0%" y="19">CON</label>
      </groupheader>
      <groupfooter name="LC_INLINE_SQL_1_pc_CICLOGroupFooter" color="black" fontname="Arial" fontsize="12" fontstyle="bold" pagebreak-before-print="false" pagebreak-after-print="false"/>
    </group>
  </groups>
</report>
