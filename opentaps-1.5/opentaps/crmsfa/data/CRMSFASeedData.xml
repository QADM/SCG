    <StatusValidChange condition="" statusId="TASK_ON_HOLD" statusIdTo="TASK_SCHEDULED" transitionName="Re-schedule"/>
    <StatusValidChange condition="" statusId="TASK_ON_HOLD" statusIdTo="TASK_CANCELLED" transitionName="Cancel"/>

