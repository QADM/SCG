    <StatusValidChange statusId="PTYLEAD_RECYCLED" statusIdTo="PTYLEAD_DEAD" transitionName="Dead"/>
    
    <StatusValidChange condition="" statusId="CRQ_SUBMITTED" statusIdTo="CRQ_COMPLETED" transitionName="Complete Request"/>

