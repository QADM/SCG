<!-- Catalog request survey seed data -->

<entity-engine-xml>

    <!-- Define a Party and Person that will take the catalog requests. This party has permission to do everything a Customer Service Rep can
         and will be the Assigned to party for any Leads created by the catalog request. -->
    <Party partyId="AUTO_REQ_TAKER" partyTypeId="PERSON"/>
    <Person partyId="AUTO_REQ_TAKER" firstName="Auto Request" lastName="Taker"/>
    <UserLogin partyId="AUTO_REQ_TAKER" userLoginId="autorqtaker" enabled="N"/>
    <UserLoginSecurityGroup userLoginId="autorqtaker" groupId="CSR" fromDate="2006-01-01 00:00:00"/>
    <PartyRole partyId="AUTO_REQ_TAKER" roleTypeId="CUST_SERVICE_REP"/>

</entity-engine-xml>
