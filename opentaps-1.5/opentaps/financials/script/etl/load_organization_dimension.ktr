    <connection>ofbiz_ods</connection>
    <sql>SELECT
  PTY.party_id AS organization_party_id,
  PG.group_name AS organization_name
FROM
  PARTY PTY
  INNER JOIN PARTY_GROUP PG ON PTY.party_id = PG.party_id
  INNER JOIN PARTY_ROLE PR ON PTY.party_id = PR.party_id
WHERE
  PR.role_type_id = &apos;INTERNAL_ORGANIZATIO&apos;

</sql>
